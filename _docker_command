C="\e[1;31m"
E="\e[0m"

echo -e "$C[DOCKER SETTING] :: apt update$E"
apt update
echo -e "$C[DOCKER SETTING] :: install git$E"
apt install -y git
echo -e "$C[DOCKER SETTING] :: clone$E"
git clone https://github.com/skyrich2000/env
cd /env
echo -e "$C[DOCKER SETTING] :: run docker setting$E"
./docker
